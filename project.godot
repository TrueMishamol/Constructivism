; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Constructivism"
config/name_localized={
"en": "Constructivism Interior Visualization",
"ru": "Визуализация интерьера в стиле Конструктивизм"
}
run/main_scene="res://levels/constructivism.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.403922, 0.443137, 0.498039, 1)
boot_splash/show_image=false
config/icon="res://visual/ui/constructivism.png"

[autoload]

ApplicationControls="*res://scripts/application_controls.gd"
Singleton="*res://scripts/singleton.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/Mirror/plugin.cfg", "res://addons/SmoothScroll/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "red",
"res://levels/": "purple",
"res://scenes/": "blue",
"res://scripts/": "green",
"res://visual/": "orange"
}

[filesystem]

import/fbx/enabled=false

